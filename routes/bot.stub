<?php

/** @var \Laravel\Lumen\Routing\Router $router */

/**
 * Do not edit this file, will overwrite at init:bot command
 */

use App\Http\Controllers\WebhookController;
use Illuminate\Http\Request;

$router->post(
	'/bot{{ botToken }}',
	function (Request $request) {
		return (new WebhookController())->post(
			$request,
			new {{ botClass }}()
		);
	}
);

$router->get('/bot{{ botToken }}', function () {
	return abort(404);
});
